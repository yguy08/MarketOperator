<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" type="BorderPane" prefWidth="320" prefHeight="570">
	<padding>
	</padding>
	
	<top>
	<HBox minHeight="54">
		<Label></Label>
	</HBox>
	
	</top>
	
	<center>
	    <ListView 
	    fx:id="listViewDisplay">
	    </ListView>
	</center>
	
	<bottom>
		<HBox minHeight="54" spacing="5">
			<Button fx:id="showAll" onAction="#showAll" minWidth="52" minHeight="52" />
			<Button fx:id="showHighs" onAction="#showHighs" minWidth="52" minHeight="52" />
			<Button fx:id="showLows" onAction="#showLows" minWidth="52" minHeight="52" />
			<Button fx:id="buy" minWidth="52" minHeight="52" />
			<Button fx:id="sell" minWidth="52" Text="Sell" minHeight="52" />
			<Button fx:id="automate" minWidth="52" minHeight="52" />
		</HBox>
	</bottom>
       
    <stylesheets><URL value="@VaultMain.css"/></stylesheets>
	
</fx:root>

