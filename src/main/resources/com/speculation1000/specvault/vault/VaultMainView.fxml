<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" type="BorderPane" prefWidth="320" prefHeight="570">
	<padding>
	</padding>
	
	<top>
	<HBox prefHeight="25">
		<Label></Label>
	</HBox>
	
	</top>
	
	<center>
	    <ListView 
	    fx:id="listViewDisplay">
	    </ListView>
	</center>
	
	<bottom>
		<HBox prefHeight="50" spacing="5">
			<Button fx:id="showAll" onAction="#showAll" prefHeight="50" prefWidth="50" text="All" />
			<Button fx:id="showHighs" onAction="#showHighs" prefHeight="50" prefWidth="50" text="H" />
			<Button fx:id="showLows" onAction="#showLows" prefHeight="50" prefWidth="50" text="L" />
			<Button fx:id="buy" onAction="#buy" prefHeight="50" prefWidth="50" text="Buy" />
			<Button fx:id="sell" onAction="#sell" prefHeight="50" prefWidth="50" text="Sell" />
			<Button fx:id="auto" onAction="#auto" prefHeight="50" prefWidth="50" text="Auto" />
		</HBox>
	</bottom>
       
    <stylesheets><URL value="@VaultMain.css"/></stylesheets>
	
</fx:root>

